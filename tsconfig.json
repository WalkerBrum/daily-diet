{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@schemas/*": [
        "./src/schemas/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@dtos/*": [
        "./src/dtos/*"
      ],
    }
  }
}
